{
  "name": "document-mgt-system",
  "version": "1.0.0",
  "description": "a full-stack document management system, complete with roles and privileges. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.\",",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "db:migrate": "sequelize db:migrate",
    "seed:test": "NODE_ENV=test babel-node server/tests/helpers/SeedHelper.js",
    "reset:test": "NODE_ENV=test babel-node server/tests/helpers/resetHelper.js",
    "seed:db": "node dist/test/server/helpers/SeedHelper.js",
    "start": "npm-run-all build seed:db start:server",
    "start:nodemon": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test:server": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/.bin/_mocha server/tests/**/*.spec.js",
    "test": "npm-run-all seed:test test:server reset:test",
    "travis": "npm-run-all seed:test test:server",
    "server": "nodemon --watch server --exec babel-node -- server.js",
    "test-e2e": "nightwatch",
    "client:test": "_mocha --reporter spec client/test/testSetup.js client/test/**/*.spec.**",
    "client:test:watch": "npm run client:test -- --watch",
    "api-documentation": "rimraf docs/ && jsdoc -r -c ./conf.json --readme ./documentation/README.md --destination docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-moseni/document-mgt-system.git"
  },
  "keywords": [
    "document",
    "user",
    "role"
  ],
  "author": "Halimat Mercy Oseni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-moseni/document-mgt-system/issues"
  },
  "homepage": "https://github.com/andela-moseni/document-mgt-system#readme",
